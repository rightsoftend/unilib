{
  "name": "@nerv_uk/unilib",
  "version": "0.0.6",
  "description": "Universal library",
  "exports": {
    "./numbers": {
      "import": "./dist/numbers.js",
      "require": "./dist/numbers.cjs",
      "types": "./dist/numbers.d.ts"
    },
    "./dates": {
      "import": "./dist/dates.js",
      "require": "./dist/dates.cjs",
      "types": "./dist/dates.d.ts"
    },
    "./strings": {
      "import": "./dist/strings.js",
      "require": "./dist/strings.cjs",
      "types": "./dist/strings.d.ts"
    },
    "./universal": {
      "import": "./dist/universal.js",
      "require": "./dist/universal.cjs",
      "types": "./dist/universal.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:cjs1": "tsc --project tsconfig.cjs.json && npm run rename:cjs",
    "rename:cjs": "find dist -name '*.js' -exec sh -c 'mv \"$0\" \"${0%.js}.cjs\"' {} \\;",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rightsoftend/unilib.git"
  },
  "keywords": [
    "universal",
    "library"
  ],
  "author": "Volodymyr Nerovnia <nerv@i.ua>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rightsoftend/unilib/issues"
  },
  "homepage": "https://github.com/rightsoftend/unilib#readme",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  }
}
